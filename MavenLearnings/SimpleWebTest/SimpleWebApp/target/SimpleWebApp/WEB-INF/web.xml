<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>MyMavenServlet</servlet-name>
  	<display-name>MyMavenServlet</display-name>
  	<description></description>
  	<servlet-class>com.spandiar.maven.web.servlet.MyMavenServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>MyMavenServlet1</servlet-name>
  	<display-name>MyMavenServlet1</display-name>
  	<description></description>
  	<servlet-class>com.spandiar.maven.web.servlet.MyMavenServlet1</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>MyMavenServlet</servlet-name>
  	<url-pattern>/MyMavenServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>MyMavenServlet1</servlet-name>
  	<url-pattern>/MyMavenServlet1</url-pattern>
  </servlet-mapping>
</web-app>
